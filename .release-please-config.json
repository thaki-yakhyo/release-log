{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "packages": {
    ".": {
      "release-type": "simple",
      "package-name": "release-log",
      "changelog-sections": [
        {
          "type": "feat",
          "section": "🚀 Features",
          "hidden": false
        },
        {
          "type": "fix",
          "section": "🐛 Bug Fixes",
          "hidden": false
        },
        {
          "type": "docs",
          "section": "📚 Documentation",
          "hidden": false
        },
        {
          "type": "perf",
          "section": "⚡ Performance",
          "hidden": false
        },
        {
          "type": "build",
          "section": "🏗️ Build",
          "hidden": false
        },
        {
          "type": "ci",
          "section": "🔧 CI/CD",
          "hidden": false
        },
        {
          "type": "style",
          "section": "🎨 Style",
          "hidden": false
        },
        {
          "type": "refactor",
          "section": "♻️ Refactoring",
          "hidden": false
        },
        {
          "type": "test",
          "section": "🧪 Testing",
          "hidden": false
        },
        {
          "type": "chore",
          "section": "🔧 Chores",
          "hidden": false
        },
        {
          "type": "security",
          "section": "🔒 Security",
          "hidden": false
        }
      ],
      "release-search-depth": 400,
      "include-component-in-tag": false,
      "include-v-in-tag": true,
      "version-file": "VERSION",
      "extra-files": [
        {
          "type": "toml",
          "path": "pyproject.toml",
          "jsonpath": "$.project.version"
        }
      ]
    }
  }
}
